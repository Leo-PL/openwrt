/dts-v1/;
#include "ar9331_tplink_tl-mr3040-v2.dts"
&spi {
	flash@0 {
		partitions {
			uboot:	partition@0 {
				reg = <0x000000 0x020000>;
			};

			partition@20000 {
				reg = <0x020000 0xfd0000>;
			};

			/delete-node/ partition@3f0000;
			art: partition@ff0000 {
				reg = <0xff0000 0x010000>;
				label = "art";
				read-only;
			};
		};
	};
};
