/dts-v1/;

#include "ar7240_tplink_tl-wr743nd-v1.dts"

&spi {
	flash@0 {
		partitions {
			uboot: partition@0 {
				reg = <0x0 0x20000>;
			};

			partition@20000 {
				reg = <0x20000 0xfd0000>;
			};

			/delete-node/ partition@3f0000;
			art: partition@ff0000 {
				reg = <0xff0000 0x10000>;
				label = "art";
				read-only;
			};
		};
	};
};
