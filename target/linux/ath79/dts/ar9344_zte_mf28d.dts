#include "ar9344.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	model = "ZTE MF28D";
	compatible = "zte,mf28d", "qca,ar9344";

	aliases {
		label-mac-device = &wmac;
		led-boot = &led_power_green;
		led-failsafe = &led_power_red;
		led-running = &led_power_green;
		led-upgrade = &led_power_red;
	};

	keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&enable_gpio_11 &enable_gpio_16>;

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
		};

		wps {
			label = "wps";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio 11 GPIO_ACTIVE_LOW>;
		};
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&enable_switch_leds>;

		wlan {
			label = "green:wlan";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};

		lan1 {
			label = "green:lan1";
			gpios = <&gpio 3 GPIO_ACTIVE_LOW>;
		};

		lan2 {
			label = "green:lan2";
			gpios = <&gpio 1 GPIO_ACTIVE_LOW>;
		};

		lan3 {
			label = "green:lan3";
			gpios = <&gpio 2 GPIO_ACTIVE_LOW>;
		};

		lan4 {
			label = "green:lan4";
			gpios = <&gpio 0 GPIO_ACTIVE_LOW>;
		};
	};

	spi-leds {
		compatible = "gpio-leds";

		led_power_red: power-red {
			label = "red:power";
			gpios = <&spi_gpio 4 GPIO_ACTIVE_LOW>;
			panic-indicator;
		};

		led_power_green: power-green {
			label = "green:power";
			gpios = <&spi_gpio 3 GPIO_ACTIVE_LOW>;
			default-state = "on";
		};

		wwan-link-blue {
			label = "blue:wwan-link";
			gpios = <&spi_gpio 2 GPIO_ACTIVE_LOW>;
		};

		wwan-link-green {
			label = "green:wwan-link";
			gpios = <&spi_gpio 6 GPIO_ACTIVE_LOW>;
		};

		wwan-signal {
			label = "green:wwan-signal";
			gpios = <&spi_gpio 1 GPIO_ACTIVE_LOW>;
		};

		wwan-activity {
			label = "green:wwan-activity";
			gpios = <&spi_gpio 5 GPIO_ACTIVE_LOW>;
		};

		phone1 {
			label = "green:phone1";
			gpios = <&spi_gpio 0 GPIO_ACTIVE_LOW>;
		};

		phone2 {
			label = "green:phone2";
			gpios = <&spi_gpio 7 GPIO_ACTIVE_LOW>;
		};
	};

	gpio-spi {
		compatible = "spi-gpio";
		#address-cells = <1>;
		#size-cells = <0>;
		ranges;
		mosi-gpios = <&gpio 20 GPIO_ACTIVE_HIGH>;
		sck-gpios = <&gpio 21 GPIO_ACTIVE_HIGH>;
		cs-gpios =  <&gpio 15 GPIO_ACTIVE_HIGH>;
		num-chipselects = <1>;

		spi_gpio: gpio-expander@1 {
			compatible = "fairchild,74hc595";
			reg = <0>;
			registers-number = <2>;
			spi-max-frequency = <24000000>;
			gpio-controller;
			#gpio-cells = <2>;
		};
	};
};

&pinmux {
	enable_gpio_11: pinctrl_enable_gpio_11 {
		pinctrl-single,bits = <0x8 0x0 0xff000000>;
	};

	enable_gpio_16: pinctrl_enable_gpio_16 {
		pinctrl-single,bits = <0x10 0x0 0xff>;
	};

	enable_switch_leds: pinctrl_enable_switch_leds {
		pinctrl-single,bits = <0x0 0x0 0xffffffff>;
	};
};

&eth1 {
	status = "okay";

	nvmem-cells = <&macaddr_mac_0>;
	nvmem-cell-names = "mac-address";
	mac-address-increment = <1>;

	gmac-config {
		device = <&gmac>;
		switch-phy-swap = <1>;
		switch-only-mode = <1>;
	};
};

&nand {
	status = "okay";

	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		partition@0 {
			label = "u-boot";
			reg = <0x0000000 0x0080000>;
			read-only;
		};

		partition@80000 {
			label = "u-boot-env";
			compatible = "u-boot,env";
			reg = <0x0080000 0x0020000>;
			read-only;
		};

		partition@a0000 {
			label = "mib0";
			reg = <0x00a0000 0x0040000>;
			read-only;
		};

		caldata: partition@d0000 {
			label = "caldata";
			reg = <0x00d0000 0x0040000>;
			read-only;
		};

		mac: partition@120000 {
			label = "mac";
			reg = <0x0120000 0x0040000>;
			read-only;
		};

		partition@160000 {
			label = "inic";
			reg = <0x0160000 0x0200000>;
			read-only;
		};

		partition@360000 {
			label = "kernel";
			reg = <0x0360000 0x0400000>;
		};

		/*
		 * This encompasses stock rootfs, web, data,
		 * flash-image, cfg-wifi and cfg-param partitions
		 */
		partition@760000 {
			label = "ubi";
			reg = <0x0760000 0x78a0000>;
		};
	};
};

&ref {
	clock-frequency = <40000000>;
};

&usb {
	status = "okay";
};

&usb_phy {
	status = "okay";
};

&wmac {
	status = "okay";

	nvmem-cells = <&macaddr_mac_0>, <&cal_caldata_1000>;
	nvmem-cell-names = "mac-address", "calibration";
};

&caldata {
	compatible = "nvmem-cells";
	#address-cells = <1>;
	#size-cells = <1>;

	cal_caldata_1000: calibration@1000 {
		reg = <0x1000 0x440>;
	};
};

&mac {
	compatible = "nvmem-cells";
	#address-cells = <1>;
	#size-cells = <1>;

	macaddr_mac_0: macaddr@0 {
		reg = <0x0 0x6>;
	};
};
