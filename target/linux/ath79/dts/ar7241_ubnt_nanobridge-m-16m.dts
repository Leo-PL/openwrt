#include "ar7241_ubnt_nanobridge-m.dts"

&spi {
	flash@0 {
		partitions {
			partition@0 {
				reg = <0x000000 0x040000>;
			};

			partition@1 {
				reg = <0x040000 0x010000>;
			};

			partition@2 {
				reg = <0x050000 0xf50000>;
			};

			partition@3 {
				reg = <0xfa0000 0x010000>;
			};

			partition@4 {
				reg = <0xfb0000 0x040000>;
			};

			art: partition@5 {
				label = "art";
				reg = <0xff0000 0x010000>;
				read-only;
			};
		};
	};
};
