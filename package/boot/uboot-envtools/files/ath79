[ -e /etc/config/ubootenv ] && exit 0

touch /etc/config/ubootenv

. /lib/uboot-envtools.sh
. /lib/functions.sh

board=$(board_name)

case "$board" in
8dev,lima)
	ubootenv_add_uci_config "/dev/mtd1" "0x0" "0x40000" "0x40000"
	;;
8dev,rambutan)
	ubootenv_add_uci_config "/dev/mtd1" "0x0" "0x300000" "0x100000"
	;;
8dev,carambola2|\
aerohive,hiveap-121|\
alfa-network,ap121f|\
alfa-network,ap121fe|\
alfa-network,n2q|\
alfa-network,n5q|\
alfa-network,pi-wifi4|\
alfa-network,r36a|\
allnet,all-wap02860ac|\
arduino,yun|\
buffalo,bhr-4grv|\
